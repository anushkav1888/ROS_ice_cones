<launch>
  
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="first_tb3"  default="tb3_0"/>
 
  <arg name="x_pos" default=" 0"/>
  <arg name="y_pos" default=" 10"/>
  <arg name="z_pos" default=" 0.0"/>
  <arg name="yaw"   default=" 0.0"/>

  <include file="$(find aws_robomaker_hospital_world)/launch/hospital.launch"/>

<param name="robot_description" command="$(find xacro)/xacro --inorder $(find turtlebot3_description)/urdf/turtlebot3_burger.urdf.xacro" />/
  <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model turtlebot3_$(arg model) -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -param robot_description" />

<!-- <node name = "main_controller" pkg="dyn_IPC_sim" type ="main_controller_coded.py" output="screen" /> -->
<!-- <node name = "multi_robot_controller" pkg="dyn_IPC_sim" type ="MultiIPC_Gazebo.py" output="screen" /> -->
</launch> 
